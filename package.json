{
  "name": "@tbrul/tooling-config",
  "description": "Tooling configs for trbul packages",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "release": "bumpp -r && pnpm -r publish",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@tbrul/eslint-config": "workspace:*",
    "@tbrul/prettier-config": "workspace:*",
    "bumpp": "^9.4.1",
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "typescript": "5.4.5"
  },
  "author": "tom-brulin",
  "license": "MIT",
  "keywords": [],
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": ["typescript", "@babel/core", "prettier"]
    }
  },
  "eslintConfig": {
    "extends": "@tbrul/eslint-config/package",
    "rules": {
      "unicorn/prefer-module": "off"
    }
  },
  "prettier": "@tbrul/prettier-config"
}
